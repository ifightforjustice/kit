extern putchar func[i32, i32];
extern flushstdout func[i32];

def print func[u32, u32] = fn(x u32) u32 {
  var printed u32 = 0u;
  if (x == 0u) {
    putchar(48);
    return 1u;
  } else {
    var acc u32 = 0u;
    var count u32 = 0u;
    while (x > 0u) {
      acc = acc * 10u + x % 10u;
      x = x / 10u;
      count = count + 1u;
    }
    while (printed < count) {
      var ch i32 = ~(48u | (acc % 10u));
      putchar(ch);
      acc = acc / 10u;
      printed = printed + 1u;
    }
    return printed;
  }
};
