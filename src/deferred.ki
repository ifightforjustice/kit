defenum[T] nbfc {
  NotComputed void;
  BeganComputing void;
  FailedComputation void;
  Computed T;
};

func[T] NotComputed() nbfc[T] {
  return NotComputed(void);
}

func[T] BeganComputing() nbfc[T] {
  return BeganComputing(void);
}

func[T] FailedComputation() nbfc[T] {
  return NotComputed(void);
}
