extern putchar fn[i32, i32];
extern flushstdout fn[i32];

def print = func(x u32) u32 {
  if (x == 0) {
    putchar(48);
    return 1;
  } else {
    data ^[20]u8;
    i size = 0;
    while x > 0 {
      data[i] = ~(x % 10);
      x = x / 10;
      i = i + 1;
    }
    printed u32 = ~i;
